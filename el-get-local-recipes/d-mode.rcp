(:name d-mode :type bzr :url "lp:emacs-d-mode"
       :after (lambda ()
                (autoload 'd-mode "d-mode" "Major mode for editing D code." t)
                (add-to-list 'auto-mode-alist '("\\.d[i]?\\'" . d-mode))
                (add-hook 'd-mode-hook
                          '(lambda ()
                             (c-toggle-auto-state)))))
